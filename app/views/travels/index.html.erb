<p id="notice"><%= notice %></p>

<h1>Travels</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Origen</th>
      <th>Destino</th>
      <th>Precio</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Asientos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travels.each do |travel| %>
      <tr>
        <td><%= travel.user.email %></td>
        <td><%= travel.origen %></td>
        <td><%= travel.destino %></td>
        <td><%= travel.precio %></td>
        <td><%= travel.fecha %></td>
        <td><%= travel.hora %></td>
        <td><%= travel.asientos %></td>
        <td><%= link_to 'Show', travel %></td>
        <td><%= link_to 'Edit', edit_travel_path(travel) %></td>
        <td><%= link_to 'Destroy', travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travel', new_travel_path %>
